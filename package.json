{
  "name": "appsyncmasterclass-backend",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "description": "Backend for the AppSync Masterclass demo app",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --ignore-path .gitignore",
    "format": "prettier --write src/",
    "test": "vitest",
    "test:integration": "vitest integration",
    "test:e2e": "vitest e2e",
    "sls": "sls",
    "export-env": "sls export-env --function confirmUserSignup",
    "deploy:dev": "sls deploy --stage dev --region us-east-1 --verbose",
    "package:dev": "sls package --stage dev --region us-east-1 --verbose",
    "remove:dev": "sls remove --stage dev --region us-east-1 --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daroleary/appsyncmasterclass-backend.git"
  },
