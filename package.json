  "devDependencies": {
    "@aws-sdk/client-lambda": "^3.470.0",
    "@rushstack/eslint-patch": "^1.6.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "autoprefixer": "^10.4.16",
    "dotenv": "^16.3.1",
    "esbuild": "^0.19.8",
    "eslint": "^8.55.0",
    "eslint-plugin-vue": "^9.19.2",
    "postcss": "^8.4.32",
